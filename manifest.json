{
  "manifest_version": 3,
  "name": "DiaBERT Classifier",
  "version": "1.2.5",
  "description": "Classify diabetes-related text and get explanations.",
  "action": {
    "default_popup": "popup.html",
    "default_title": "DiaBERT Misinformation Classifier"
  },
  "host_permissions": [
    "https://diabert.fly.dev/*"
  ],
  "permissions": [],
  "icons": {
  "16": "icons/icon16.png",
  "48": "icons/icon48.png",
  "128": "icons/icon128.png"
  }

}
